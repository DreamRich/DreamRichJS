{
  "name": "dreamrich",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.4",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "date-fns": "^1.28.5",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "1.1.3",
    "flux": "^3.1.3",
    "formsy-material-ui": "^0.6.3",
    "formsy-react": "^0.19.5",
    "fs-extra": "4.0.2",
    "highcharts": "^6.0.1",
    "html-webpack-plugin": "2.30.1",
    "material-ui": "^0.19.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-data-grid": "^2.0.58",
    "react-data-grid-addons": "^2.0.58",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-flexbox-grid": "^1.1.5",
    "react-responsive": "^3.0.0",
    "react-router-dom": "^4.1.2",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "uglify-js": "^2.8.29",
    "underscore": "^1.8.3",
    "url-loader": "0.5.9",
    "webpack": "3.6.0",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "8.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.7.2",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.37.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "jest": "21.2.1",
    "node-sass": "^4.5.3",
    "npm-check-updates": "^2.12.1",
    "react-dev-utils": "^3.0.2",
    "sass-loader": "^6.0.6",
    "webpack-dev-server": "2.9.1"
  },
  "scripts": {
    "preinstall": "npm run dreamrich",
    "preuninstall": "npm run dreamrich",
    "pretest": "npm run dreamrich",
    "prestart": "npm run dreamrich",
    "prebuild": "npm run dreamrich",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "coveralls": "npm test -- --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "dreamrich": "cat ./dreamrich"
  },
  "proxy": {
    "/api": {
      "target": "http://0.0.0.0:8000"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
